{
  "name": "lemec93/laravel-helpers",
  "description": "Provided helpers function and some helper class.",
  "keywords": ["laravel", "helpers"],
  "license": "MIT",
  "authors": [
    {
      "name": "DJAMA Med",
      "email": "d.lemec93@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": ">=5.2.0",
    "guzzlehttp/guzzle": "~6.0",
    "tymon/jwt-auth": "0.5.* || 1.0.*",
    "minime/annotations": "2.3.1",
    "maatwebsite/excel": "3.*",
    "tecnick.com/tcpdf": "~6.0.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Lemec93\\Support\\Tests\\": "tests/",
      "Lemec93\\Support\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "files": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Lemec93\\Support\\HelpersServiceProvider"
      ]
    }
  }
}
